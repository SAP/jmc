<jfragent>
    <config>
        <classprefix>__JFREvent</classprefix>
        <allowtostring>false</allowtostring>
        <allowconverter>true</allowconverter>
    </config>
    <events>
        <event id="jdk.log.sysPropsChanged">
            <label>sysPropsChanged</label>
            <description>Change of system property</description>
            <class>java.util.Properties</class>
            <stacktrace>false</stacktrace>
            <rethrow>false</rethrow>
            <path>jdk/log/sysProps/changed</path>
            <method>
                <name>put</name>
                <descriptor>(Ljava/lang/Object;Ljava/lang/Object;)Ljava/lang/Object;</descriptor>
                <parameters>
                    <parameter index="0">
                        <name>key</name>
                        <description>The key</description>
                        <converter>org.openjdk.jmc.agent.converters.sap.SystemPropChangeLogger.logKey(Ljava/lang/String;)Z</converter>
                    </parameter>
                    <parameter index="1">
                        <name>value</name>
                        <description>The changed value</description>
                        <converter>org.openjdk.jmc.agent.converters.sap.SystemPropChangeLogger.logValue(Ljava/lang/String;)Z</converter>
                    </parameter>
                </parameters>
            </method>
            <fields>
                <field>
                    <name>properties</name>
                    <expression>this</expression>
                    <description>The properties object</description>
                    <converter>org.openjdk.jmc.agent.converters.sap.SystemPropChangeLogger.logProperties(Ljava/util/Properties;)Z</converter>
                </field>
            </fields>
        </event>
        <event id="jdk.log.sysPropsChanged">
            <label>sysPropsRemoved</label>
            <description>Removing of system property</description>
            <class>java.util.Properties</class>
            <stacktrace>false</stacktrace>
            <rethrow>false</rethrow>
            <path>jdk/log/sysProps/changed</path>
            <method>
                <name>remove</name>
                <descriptor>(Ljava/lang/Object;)Ljava/lang/Object;</descriptor>
                <parameters>
                    <parameter index="0">
                        <name>key</name>
                        <description>The key</description>
                        <converter>org.openjdk.jmc.agent.converters.sap.SystemPropChangeLoggerKeyConverter</converter>
                    </parameter>
                </parameters>
            </method>
            <fields>
                <field>
                    <name>properties</name>
                    <expression>this</expression>
                    <description>The properties object</description>
                    <converter>org.openjdk.jmc.agent.converters.sap.SystemPropChangeLoggerPropsConverter</converter>
                </field>
            </fields>
        </event>
    </events>
</jfragent>
